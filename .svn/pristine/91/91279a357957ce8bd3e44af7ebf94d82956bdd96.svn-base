	<select id="getList" parameterType="Map" resultMap="BaseResultMap">
		SELECT
		hydw,xmmc,xmnf,cyxmkgsj,jgsj,zyd
		FROM frk.cg_gc_zdcyxm_valid
		WHERE frwybs = #{frwybs} ORDER BY xmnf DESC
	</select>

	<select id="selectZdcyDetail" parameterType="Map" resultMap="BaseResultMap">
		Select frwybs,xmmc,hydw,xmnf,cyxmkgsj,jgsj,zyd from frk.cg_gc_zdcyxm_valid
		<where>
			<if test="xmmc!=null and xmmc!=''">
				AND xmmc LIKE '%'||#{xmmc}||'%'
			</if>
			<if test="startTime!=null and startTime !=''">
				AND xmnf &gt;=#{startTime}
			</if>
			<if test="endTime!=null and endTime !=''">
				AND xmnf &lt;=#{endTime}
			</if>
		</where>
