	<select id="selectDetail" parameterType="java.util.Map" resultMap="BaseResultMap">
		SELECT frmc,frwybs,tyxydm,fddbr,djrq,hydm,cxzt
		From frk.ca_dj_jbxx
		<where>
			<if test="LPname != null and LPname != ''">
				AND frmc LIKE '%'||#{LPname}||'%'
			</if>
			<if test="Range != null and Range != ''">
				AND ywfw LIKE '%'||#{Range}||'%'
			</if>
			<if test="TrustCode != null and TrustCode != ''">
				AND tyxydm = #{TrustCode}
			</if>
		</where>
	</select>


