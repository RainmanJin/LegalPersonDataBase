       */
      function initDataTable0() {
        entTable0 = dataTable.load({
          // 需要初始化dataTable的dom元素
          el: '#search_table0',
          showIndex: true,
          scrollX: true,
          aLengthMenu: [5, 10, 20, 50, 100],
          ajax: {
            url: '/data/search/taxsearch0.json',
            data: function (d) {
              d.params = $("#search_form0").serializeObject();
            }
          },
          columns: [  {data: null, defaultContent: 1, },
                      {data: "frmc" , cut: {length: 20, repalce: '...'}},
			          { data: "sssq"  },
			          { data: "gsyje" },
			          { data: "gssje" }],
          columnDefs: [
            {
              targets: 1,
              render: function (data, type, row, meta) {
                return "<a href='javascript:void(0)' class='frmc0' priPID='" + row.frwybs + "'>" + row.frmc + "</a>";
              }
            },
            {
                targets: 3,
                render: function (data, type, row, meta) {
                	if(superNo == 'adminfrk'){ return row.gsyje; }
                	else{ return '***'; }   
                }
            },
            {
                targets: 4,
                render: function (data, type, row, meta) {
                	if(superNo == 'adminfrk'){ return row.gssje; }
                	else{ return '***'; }
                }
              }
            ]
        });
        entTable0.on('xhr.dt', function (e, settings, json) {
          if (json.recordsTotal <= 5) {
            $('#search_table0_wrapper .bottom').hide();
          } else {
            $('#search_table0_wrapper .bottom').show();
          }
        })
      }

