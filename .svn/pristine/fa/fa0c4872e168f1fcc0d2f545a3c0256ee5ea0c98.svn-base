	<select id="selectList" parameterType="String" resultMap="BaseResultMap">
		SELECT syqdjrq,qszt,syq_nx,bz_dy,bz_cf,syzmc
		From frk.cd_wxzc_gtsyqxx_valid
		WHERE frwybs= #{frwybs}
	</select>
	<select id="selectListByMap" parameterType="Map" resultMap="BaseResultMap">
		SELECT a.syqdjrq,a.qszt,a.syq_nx,a.bz_dy,a.bz_cf,a.syzmc,b.frmc,b.frwybs
		From frk.cd_wxzc_gtsyqxx_valid a, frk.ca_dj_jbxx b
		<where>
			a.frwybs = b.frwybs
			<if test="UserName != null and UserName != ''">
				AND a.syzmc like '%'||#{UserName}||'%'
			</if>
			<if test="Use_Date_Start != null and Use_Date_Start != ''">
				AND a.syqdjrq &gt;= cast(CONCAT(#{Use_Date_Start},' 00:00:00') as TIMESTAMP)
			</if>
			<if test="Use_Date_End != null and Use_Date_End != ''">
				AND a.syqdjrq &lt;= cast(CONCAT(#{Use_Date_End},' 23:59:59')as TIMESTAMP)
			</if>

