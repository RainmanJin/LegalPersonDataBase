	<!--获取法人登记基本信息列表-->
	<select id="selectList" parameterType="java.util.Map" resultMap="BaseResultMap">
		SELECT
		frwybs,tyxydm,zzjgdm,frfl,frflxl,frmc,fddbr,fr_zjlx,fr_zjhm,djjg,zgjg,djrq,cxzt,cyrs,
		djzlx,djzch,ywfw,qy_jy,zczb,bm_bz,jfly,jgsyxz,shzzlx,qy_lx,qy_lxdl,hyml,hydm,hzrq,jyqsrq,
		jyjzrq,jggss,jgsws,gt_zcxs,bz_cz,zjjb,zjpb,mzbm_ns,jb,bz_zfzg,jyjg_bxlx,whjg_dj,
		dh,yx,cz,frmc_jc,frmc_en,frjgdm,zcdz
		FROM frk.ca_dj_jbxx
		<where>
			<if test="frmc != null and frmc != ''">
				AND frmc LIKE '%'||#{frmc}||'%'
			</if>
			<if test="ywfw != null and ywfw != ''">
				AND ywfw LIKE '%'||#{ywfw}||'%'
			</if>
			<if test="tyxydm != null and tyxydm != ''">
				AND tyxydm = #{tyxydm}
			</if>
			<if test="zzjgdm != null and zzjgdm != ''">
				AND zzjgdm = #{zzjgdm}
			</if>
			<if test="djzch != null and djzch != ''">
				AND djzch = #{djzch}
			</if>
			<if test="hydm != null and hydm != ''">
				AND hydm = #{hydm}
			</if>
			<if test="cxzt != null and cxzt != ''">
				AND cxzt = #{cxzt}
			</if>
		</where>

	</select>

	<!--获取法人登记基本信息列表-->
	<select id="selectListByKeyword" parameterType="java.util.Map" resultMap="BaseResultMap">
		SELECT
		frwybs,frmc,tyxydm,djrq,fddbr,djzch
		FROM frk.ca_dj_jbxx
		<where>
			<if test="frmc != null and frmc != ''">
				OR frmc LIKE '%'||#{frmc}||'%'
			</if>
			<if test="tyxydm != null and tyxydm != ''">
				OR tyxydm LIKE '%'||#{tyxydm}||'%'
			</if>
			<if test="djzch != null and djzch != ''">
				OR djzch LIKE '%'||#{djzch}||'%'
			</if>
			<if test="zzjgdm != null and zzjgdm != ''">
				OR zzjgdm LIKE '%'||#{zzjgdm}||'%'
			</if>
			<!--<if test="nsrsbh != null and nsrsbh != ''">-->
			<!--OR nsrsbh LIKE '%'||#{nsrsbh}||'%'-->
			<!--</if>-->
		</where>

	</select>

	<!-- 法人校核  -->
	<select id="frCheck" resultType="com.icinfo.frk.api.dto.FrCheckDto">
		select frwybs,tyxydm,qydwmc,zzjgdm,hzjg,hzsj,fddbr,frlx,zt,zcdz from (
		SELECT frwybs,
		tyxydm,
		frmc as qydwmc,
		zzjgdm,
		djjg as hzjg,
		hzrq as hzsj,
		fddbr,
		(SELECT fr_content FROM frk.ca_code_frfl WHERE fr_code = frfl) frlx,
		(SELECT fr_content FROM frk.ca_code_frdjzt WHERE fr_code = cxzt) zt,
		zcdz,
		updatetime
		FROM frk.ca_dj_jbxx
		<where>
			<if test="tyxydm != null and tyxydm != ''">
				AND tyxydm = #{tyxydm}
			</if>
			<if test="frdwmc != null and frdwmc != ''">
				AND frmc LIKE '%'||#{frdwmc}||'%'
			</if>
			<if test="zzjgdm != null and zzjgdm != ''">
				AND zzjgdm = #{zzjgdm}
			</if>
		</where>
		limit 100
		)

	</select>

	<select id="selectFrHighDetail" parameterType="Map" resultMap="BaseResultMap">
		Select frwybs,cxzt,tyxydm,frmc,fddbr,djrq,zczb,qy_lxdl,hydm,zcdz,datasrdep,djzch from
		frk.ca_dj_jbxx
		<where>
			<if test="frmc!=null and frmc!=''">
				AND frmc LIKE '%'||#{frmc}||'%'
			</if>
			<if test="tyxydm!=null and tyxydm!=''">
				AND tyxydm=#{tyxydm}
			</if>
			<if test="cxzt!=null and cxzt!=''">
				AND cxzt=#{cxzt}
			</if>
			<if test="frfl!=null and frfl!=''">
				AND frfl=#{frfl}
			</if>
			<if test="datasrdep!=null and datasrdep!=''">
				AND datasrdep=#{datasrdep}
			</if>
			<if test="startTime!=null and startTime !=''">
				AND djrq &gt;=cast(concat(#{startTime}, ' 00:00:00') as TIMESTAMP)
			</if>
			<if test="endTime!=null and endTime !=''">
				AND djrq &lt;=cast(concat(#{endTime}, ' 23:59:59') as TIMESTAMP) 
			</if>
			<if test="fddbr!=null and fddbr!=''">
				AND fddbr=#{fddbr}
			</if>
			<if test="hydm!=null and hydm!=''">
				AND hydm in
				<foreach close=")" collection="hydmStr" index="index" item="item" open="(" separator=",">
					#{item}
				</foreach>
			</if>
			<if test="zczb!=null and zczb!=''">
				AND zczb=CAST(#{zczb} as decimal)
			</if>
		</where>
	</select>

	<!--获取法人登记基本信息-->
	<select id="GetOne" parameterType="String" resultMap="BaseResultMap1">
		SELECT frmc,cxzt,tyxydm,zczb,djzch
		From frk.ca_dj_jbxx
		WHERE frwybs= #{frwybs}
	</select>

	<!--获取法人登记基本信息与状态-->
	<select id="GetOneWithStatus" parameterType="String" resultMap="BaseResultMap1">
		SELECT frmc,cxzt,tyxydm,zczb,fddbr,djrq,jyqsrq,jyjzrq,ywfw,djjg,fr_content,hydm,djzch
		From frk.ca_dj_jbxx a
		LEFT JOIN frk.ca_code_frfl b ON a.frfl= b.fr_code
		WHERE a.frwybs= #{frwybs}
	</select>

	<!--获取法人登记基本信息-->
	<select id="getPerDetail" parameterType="String" resultMap="BaseResultMap">
		SELECT tyxydm,frwybs,frmc,djrq,djjg,cxzt,djzch
		From frk.ca_dj_jbxx
		WHERE frwybs= #{frwybs}
	</select>

	<select id="selectListByMap" parameterType="java.util.Map"
		resultMap="BaseResultMap2">
		SELECT
		frwybs,frmc,zczb
		FROM frk.ca_dj_jbxx
		<where>
			<if test="assetL != null and assetL != ''">
				AND zczb &gt;= cast(#{assetL} as INT)
			</if>
			<if test="assetH != null and assetH != ''">
				AND zczb &lt;= cast(#{assetH} as INT)
			</if>
			<if test="frmc != null and frmc != ''">
				AND frmc like '%'||#{frmc}||'%'
			</if>
		</where>

	</select>


	<select id="getCaDjJbxx" parameterType="Map" resultMap="BaseResultMap1">
		SELECT A .frmc,A .frwybs,A .fddbr,A .djjg,A .djrq,A .jyqsrq,A .jyjzrq,A .ywfw,A
		.hydm,b.fr_content,C .zs
		FROM frk.ca_dj_jbxx A
		LEFT JOIN frk.ca_code_frfl b ON A .frfl = b.fr_code
		LEFT JOIN frk.ca_cs_dzxx_valid C ON A .frwybs = C .frwybs
		<where>
			<if test="frmc != null and frmc != ''">
				and frmc like '%'||#{frmc}||'%'
			</if>
			<if test="fddbr != null and fddbr != ''">
				and fddbr like '%'||#{fddbr}||'%'
			</if>
		</where>
	</select>

	<select id="GetAssetDto" parameterType="String" resultMap="BaseResultMap2">
		SELECT frmc,cxzt,tyxydm,zczb,djzch,bm_bz
		From frk.ca_dj_jbxx
		WHERE frwybs= #{frwybs}
	</select>

