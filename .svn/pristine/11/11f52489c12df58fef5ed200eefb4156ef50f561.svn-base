	<select id="getList" parameterType="String" resultMap="BaseResultMap">
		SELECT
		tbdh,hbcm,bxqq,bxzq,bbr,syr
		FROM frk.cf_sb_cdhbxx_valid
		WHERE frwybs = #{frwybs}
	</select>

	<select id="selectListByMap" parameterType="Map" resultMap="BaseResultMap">
		SELECT
		a.tbdh,a.hbcm,a.bxqq,a.bxzq,a.bbr,a.syr,b.frmc,b.frwybs
		FROM frk.cf_sb_cdhbxx_valid a, frk.ca_dj_jbxx b
		<where>
			a.frwybs = b.frwybs
			<if test="BoatName != null and BoatName != ''">
				AND a.hbcm like '%'||#{BoatName}||'%'
			</if>
			<if test="ProtectedName != null and ProtectedName != ''">
				AND a.bbr like '%'||#{ProtectedName}||'%'
			</if>
		</where>
	</select>
